{
  "POS fixes to Compromise output": {
    "order": "1",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": { "file": "fixes" }
  },

  "Prepare Document": {
    "order": "2",
    "action": "process",
    "parseBy": "process",
    "scope": "document",
    "source": "file",
    "payload": { "file": "prepare" }
  },

  "Compound Terms": {
    "order": "3",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "directory",
    "payload": { "directory": "compound-terms" }
  },

  "Disambiguate 'to'": {
    "order": "8",
    "action": "disambiguate",
    "parseBy": "pattern",
    "scope": "phrase",
    "source": "payload",
    "payload": { "pattern": "to", "POSes": ["preposition", "infinitive"] }
  },

  "Tag Infinitives": {
    "order": "9",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": { "file": "infinitives" }
  },

  "Tag Progressive Verbs & Verbals": {
    "order": "10",
    "action": "process",
    "parseBy": "process",
    "scope": "document",
    "source": "payload",
    "payload": { "process": "ingVerbals" }
  },

  "Disambiguate Progressive Verbals": {
    "order": "11",
    "action": "disambiguate",
    "parseBy": "pattern",
    "scope": "phrase",
    "source": "payload",
    "payload": {
      "pattern": "#ProgressiveVerbal",
      "POSes": [
        "verbalNoun",
        "presentProgressive",
        "presentParticiple",
        "gerund"
      ]
    }
  },

  "Untag disambiguated ProgressiveVerbals": {
    "order": "12",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": { "file": "remove-progressiveVerbals" }
  },

  "Disambiguate Adverbs & Propositions": {
    "order": "13",
    "action": "disambiguate",
    "parseBy": "term",
    "scope": "phrase",
    "source": "file",
    "payload": { "file": "adverb-preposition" }
  },

  "Disambiguate Nouns, Verbs, Adjectives & Adverbs through Wordnet": {
    "order": "14",
    "action": "disambiguate",
    "parseBy": "term",
    "scope": "phrase",
    "source": "file",
    "payload": { "file": "wordnet" }
  }
}
