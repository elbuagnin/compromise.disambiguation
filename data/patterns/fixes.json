{
  "Mistagged Gerunds": {
    "batchOrder": "1",
    "order": "3",
    "term": ".",
    "pattern": "(#Auxiliary && #Verb && /ing$/)",
    "tag": "#ProgressiveVerbal"
  },

  "Unset #Gerunds": {
    "batchOrder": "1",
    "term": "#Gerund",
    "pattern": "#Gerund",
    "untag": "#Gerund"
  },

  "Activity Is Verbal": {
    "batchOrder": "1",
    "term": "#Activity",
    "pattern": "(#Activity && /ing$/)",
    "tag": "#ProgressiveVerbal",
    "untag": ["#Activity", "#Noun"]
  },

  "#Auxiliary #Adverb": {
    "batchOrder": "1",
    "term": "#Adverb",
    "pattern": "(#Auxiliary && #Adverb)",
    "untag": "#Auxiliary"
  }
}
