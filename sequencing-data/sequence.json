{
  "Initial Document Processing": {
    "order": "1",
    "action": "sub-sequence",
    "payload": { "file": "pre-processing" }
  },

  "Compound Nouns": {
    "order": "2",
    "action": "process",
    "payload": { "process": "compound-nouns" }
  },

  "Initial Document Pattern Tagging": {
    "order": "3",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "directory",
    "payload": { "directory": "pre-processing" }
  },

  "Compound Terms": {
    "order": "4",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "directory",
    "payload": { "directory": "compound-terms" }
  },

  "Phrase Breaks": {
    "order": "5",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": {
      "file": "phrase-breaks"
    }
  },

  "Infinitives": {
    "order": "6",
    "action": "sub-sequence",
    "payload": { "file": "infinitives" }
  },

  "Perfect Participles": {
    "order": "7",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": {
       "file": "perfect-participles" 
    }
  },

  "Past Participles": {
    "order": "8",
    "action": "sub-sequence",
    "payload": { "file": "past-participles"}
  },

  "Progressive Verbs & Verbals": {
    "order": "9",
    "action": "sub-sequence",
    "payload": { "file": "progressive-verbals" }
  },

  "Term Lists": {
    "order": "10",
    "action": "discern",
    "parseBy": "term",
    "scope": "phrase",
    "source": "directory",
    "payload": { "directory": "." }
  },

  "Finish": {
    "order": "11",
    "action": "sub-sequence",
    "payload": { "file": "finish" }
  }
}
