{
  "Initial Document Processing": {
    "order": "1",
    "action": "sub-sequence",
    "payload": { "file": "pre-processing" }
  },

  "Initial Document Pattern Tagging": {
    "order": "2",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "directory",
    "payload": { "directory": "pre-processing" }
  },

  "Compound Terms": {
    "order": "3",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "directory",
    "payload": { "directory": "compound-terms" }
  },

  "Phrase Breaks": {
    "order": "4",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": {
      "file": "phrase-breaks"
    }
  },

  "Infinitives": {
    "order": "5",
    "action": "sub-sequence",
    "payload": { "file": "infinitives" }
  },

  "Progressive Verbs & Verbals": {
    "order": "6",
    "action": "sub-sequence",
    "payload": { "file": "progressive-verbals" }
  },

  "Term Lists": {
    "order": "7",
    "action": "discern",
    "parseBy": "term",
    "scope": "phrase",
    "source": "directory",
    "payload": { "directory": "." }
  },

  "Clean up": {
    "order": "8",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": { "file": "cleanup" }
  }
}
