{
  "infinitive negative 1": {
    "classification": "infinitive",
    "type": "negative",
    "pattern": "(#Pronoun|#Possessive|#Determiner) %word%",
    "scope": "phrase"
  },

  "allow access": {
    "classification": "infinitive",
    "type": "negative",
    "pattern": "#Infinitive (%word% && #Infinitive)",
    "scope": "phrase"
  },

  "infinitive improbable 1": {
    "classification": "infinitive",
    "type": "improbable",
    "pattern": "(!#BaseVerb *|!to) (%word% && !to)",
    "scope": "phrase"
  },

  "to be": {
    "classification": "infinitive",
    "type": "probable",
    "pattern": "(to && #Infinitive) %word%",
    "scope": "phrase"
  },

  "infinitive probable 2": {
    "classification": "infinitive",
    "type": "probable",
    "pattern": "#Infinitive #Noun %word% #Preposition",
    "scope": "phrase"
  },

  "to boldly go": {
    "classification": "infinitive",
    "type": "positive",
    "pattern": "(to && #Infinitive) #Adverb %word%",
    "scope": "phrase"
  },

  "infinitive 3": {
    "classification": "infinitive",
    "type": "positive",
    "pattern": "#BaseVerb #Noun (%word% && #PresentTense)",
    "scope": "phrase"
  },

  "infinitive 4": {
    "classification": "infinitive",
    "type": "positive",
    "pattern": "#BaseVerb #Pronoun (%word% && #PresentTense)",
    "scope": "phrase"
  },

  "infinitive 5": {
    "classification": "infinitive",
    "type": "positive",
    "pattern": "#BaseVerb #Possessive #Noun (%word% && #PresentTense)",
    "scope": "phrase"
  },

  "infinitive 6": {
    "classification": "infinitive",
    "type": "positive",
    "pattern": "#Infinitive #Noun (%word% && #PresentTense)",
    "scope": "phrase"
  },

  "to walk": {
    "classification": "infinitive",
    "type": "positive",
    "pattern": "(#Infinitive && to) (%word% && #BaseVerb)",
    "scope": "phrase"
  }
}
