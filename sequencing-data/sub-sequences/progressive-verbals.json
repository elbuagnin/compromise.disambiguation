{
  "Tag words ending in 'ing' that are Progressive Verbs & Verbals": {
    "order": "1",
    "action": "process",
    "parseBy": "process",
    "scope": "document",
    "source": "payload",
    "payload": { "process": "ing-verbals" }
  },

  "Disambiguate tagged Progressive Verbals": {
    "order": "2",
    "action": "discern",
    "parseBy": "pattern",
    "scope": "phrase",
    "source": "payload",
    "payload": {
      "pattern": "#ProgressiveVerbal",
      "classifications": [
        "verbalNoun",
        "presentProgressive",
        "presentParticiple",
        "gerund"
      ]
    }
  },

  "Post Assignment Impacts": {
    "order": "3",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "phrase",
    "source": "file",
    "payload": {
      "file": "progressive-verbals-pai"
    }
  },

  "Untag disambiguated ProgressiveVerbals": {
    "order": "4",
    "action": "tag",
    "parseBy": "pattern",
    "scope": "document",
    "source": "file",
    "payload": { "file": "remove-progressiveVerbals" }
  }
}
