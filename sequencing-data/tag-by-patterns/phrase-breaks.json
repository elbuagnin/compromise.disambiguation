{
  "Commas": {
    "order": "1",
    "term": ".",
    "pattern": "#Comma",
    "tag": "#PhraseBreak",
    "discern": false
  },

  "Subordinating Conjunctions": {
    "order": "2",
    "term": "^.",
    "pattern": ". #SubordinatingConjunction+",
    "tag": "#PhraseBreak",
    "discern": false
  },

  "Relative Pronouns": {
    "order": "2",
    "term": "^.",
    "pattern": ". #RelativePronoun+",
    "tag": "#PhraseBreak",
    "discern": false
  }
}
